    <!-- Points Policy -->
    <string name="points_policy_title">POINTS POLICY</string>
    <string name="what_is_kat">What is KAT?</string>
    <string name="kat_definition">KAT is the reward point unit of KATINAT\n1 KAT = 10,000 VND.</string>

    <string name="point_types">Types of Points (KAT)</string>
    <string name="point_types_description">Currently, KATINAT has two types of points:</string>
    <string name="point_types_accumulation">1. Membership Points\nFunction: Used for membership ranking.</string>
    <string name="point_types_bonus">2. Accumulated Points: Used to redeem discount codes.</string>

    <string name="rounding_rules">Rounding Rules</string>
    <string name="rounding_rules_description">+ 0.5* rounds down [e.g., 74,000 VND = 7 KAT].\n+ ‚â• 0.5 rounds up [e.g., 75,000 VND = 8 KAT].</string>

    <string name="how_to_earn_points">How to Earn Points</string>
    <string name="how_to_earn_points_description">Earn points through one of the following methods:</string>
    <string name="how_to_earn_points_method1">1. Scan the QR code using Momo scanner at the store.</string>
    <string name="how_to_earn_points_method2">2. Provide your registered phone number to the store staff, points will be credited within 30 minutes.</string>

    <string name="point_accumulation_rules">Points Accumulation Rules</string>
    <string name="point_accumulation_rules_description">KATINAT only supports retroactive point accumulation within 24 hours from the time of purchase.</string>

    <!-- Ranking Policy -->
    <string name="rank_policy_title">RANKING POLICY</string>
    <string name="rank_policy_heading">RANKING POLICY</string>
    <string name="rank_policy_description">Applicable to Silver, Gold, and Black Diamond members.\n* Black Diamond was previously called Platinum.</string>

    <string name="rank_evaluation_time">Ranking Evaluation Period</string>
    <string name="rank_evaluation_description">Within 03 months from the date you rank up or down.</string>
    <string name="rank_evaluation_case">Case: If you were previously ranked Silver, Gold, or Platinum in the old system, your ranking evaluation period starts from the day you register your KATINAT APP account.</string>

    <string name="how_to_keep_rank">How to Maintain Your Rank?</string>
    <string name="how_to_keep_rank_description">During the 03-month period, members must make at least one transaction to maintain their rank.</string>

    <string name="rank_drop_rules">Rules for Rank Demotion</string>
    <string name="rank_drop_rules_keep_points">1. Accumulated points: Remain unaffected.</string>
    <string name="rank_drop_rules_demote">2. Membership points: Will be reset to the highest point of the lower tier.</string>
    <string name="rank_drop_rules_silver">+ If demoted from Silver, the account will reset to 99 points as a New Member.</string>
    <string name="rank_drop_rules_gold">+ If demoted from Gold, the account will reset to 499 points as a Silver Member.</string>
    <string name="rank_drop_rules_black_diamond">+ If demoted from Black Diamond, the account will reset to 999 points as a Gold Member.</string>

